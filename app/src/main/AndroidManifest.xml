<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.go4lunch">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application

        android:name="androidx.multidex.MultiDexApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_hot_food_in_a_bowl__2_"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_hot_food_in_a_bowl__2_"
        android:supportsRtl="true"
        android:theme="@style/Theme.Go4Lunch">
        <activity
            android:name=".Authentification"
            android:label="@string/app_name"
            android:theme="@style/Theme.Go4Lunch.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"
            android:theme="@style/Theme.Go4Lunch.NoActionBar"></activity>
        <meta-data android:name="com.facebook.sdk.ApplicationId" android:value="@string/facebook_application_id"/>
    </application>

</manifest>